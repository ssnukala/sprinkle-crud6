{
    "name": "ssnukala/sprinkle-crud6",
    "type": "userfrosting-sprinkle",
    "description": "CRUD Module for UserFrosting 6.0 - Generic API CRUD Layer with JSON schema support",
    "keywords": [
        "php user management",
        "userfrosting",
        "sprinkle",
        "CRUD",
        "API",
        "JSON schema"
    ],
    "homepage": "https://github.com/ssnukala/sprinkle-crud6",
    "license": "MIT",
    "authors": [
        {
            "name": "Srinivas Nukala",
            "homepage": "https://srinivasnukala.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "userfrosting/framework": "^6.0",
        "userfrosting/sprinkle-core": "^6.0",
        "userfrosting/sprinkle-account": "^6.0",
        "userfrosting/sprinkle-admin": "^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.1",
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.2",
        "fakerphp/faker": "^1.17"
    },
    "minimum-stability": "beta",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "UserFrosting\\Sprinkle\\CRUD6\\": "app/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UserFrosting\\Sprinkle\\CRUD6\\Tests\\": "app/tests/"
        }
    },
    "archive": {
        "exclude": [
            ".archive",
            ".devcontainer",
            ".github",
            ".git",
            ".gitignore",
            "docs",
            "node_modules",
            "vendor",
            "*.tgz",
            "*.log",
            "package.json",
            "package-lock.json",
            "tsconfig.json",
            "tsconfig.node.json",
            "vite.config.ts",
            "env.d.ts",
            "phpunit.xml"
        ]
    }
}